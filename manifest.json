{
	"name":"crawerplug",
	"version":"1.0.1",
	"manifest_version":2,
	"description":"get data by browser",
	"permissions": [
    	"*://*/*","tabs","cookies","webNavigation","notifications"
  	],
    "browser_action": {
    	"default_title":"crawerplug",
    	"default_icon": "image/icon.png"
  	},
  	"background":{"scripts":["background.js"]},
  	"content_scripts":[{
		"matches":["http://www.gsxt.gov.cn/*"],
		"js":["jquery-2.0.0.min.js", "jquery-ui.min.js" ,"content.js"]
	}]
}