{
	"name":"crawerplug",
	"version":"1.0.3",
	"manifest_version":2,
	"description":"get data by browser",
	"permissions": [
		"activeTab",
		"webRequest",
		"webNavigation",
		"webRequestBlocking",
		"proxy",
		"*://*/*",
		"tabs",
		"storage",
		"unlimitedStorage"
  	],
  "browser_action": {
    	"default_title":"crawerplug",
    	"default_icon": "image/icon.png"
  	},
  "background":{"scripts":["background.js"]},
  "content_scripts":[{
		"matches":["http://www.gsxt.gov.cn/index.html"],
		"js":["jquery-2.0.0.min.js" ,"content.js"]
	}]
}
